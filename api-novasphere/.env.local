# Configurações do Banco de Dados
DATABASE_URL="postgresql://test:test@test:test/postgres?pgbouncer=true"

# Direct connection to the database. Used for migrations.
DIRECT_URL="postgresql://test:test@test:test/postgres"

# Configurações do JWT
JWT_SECRET="test"
JWT_EXPIRATION="test"

# Configurações do Supabase
SUPABASE_URL="test"
SUPABASE_KEY="test"
SUPABASE_JWT_SECRET="sua_chave_jwt_do_supabase"

# Configurações do Twilio (para WhatsApp)
TWILIO_ACCOUNT_SID="seu_account_sid_do_twilio"
TWILIO_AUTH_TOKEN="seu_auth_token_do_twilio"
TWILIO_WHATSAPP_NUMBER="seu_numero_whatsapp_do_twilio"

# Configurações do Redis (para WebSocket)
REDIS_HOST="localhost"
REDIS_PORT="6379"
REDIS_PASSWORD="sua_senha_do_redis"

# Configurações da Aplicação
PORT=3000
NODE_ENV="development"
CORS_ORIGIN="http://localhost:3000"

# Configurações de Upload (opcional, se usar Supabase Storage)
UPLOAD_FOLDER="./uploads"
MAX_FILE_SIZE="5242880"